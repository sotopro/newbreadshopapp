    "react": "18.0.0",
    "react-native": "0.69.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "eslint": "^8.21.0",
    "eslint-config-universe": "^11.1.0",
    "prettier": "^2.7.1"
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "private": true
}
